{
    "name": "FreeMarker2025",
    "displayName": "Free Marker 2025",
    "description": "Syntax highlight for FreeMarker",
    "version": "0.0.6",
    "publisher": "pschlup",
    "repository": {
        "type": "git",
        "url": "https://github.com/pschlup/vs-freemarker-2025.git"
    },
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ftl",
                "aliases": [
                    "FreeMarker",
                    "ftl"
                ],
                "extensions": [
                    ".ftl",
                    ".ftlh"
                ],
                "configuration": "./ftl.configuration.json"
            },
            {
                "id": "html",
                "aliases": [
                    "HTML",
                    ".html"
                ],
                "extensions": [
                    ".html"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ftl",
                "scopeName": "text.html.ftl",
                "path": "./syntaxes/ftl.tmLanguage"
            },
            {
                "language": "ftl",
                "scopeName": "text.ftl.html",
                "path": "./syntaxes/ftl.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "ftl",
                "path": "./snippets/ftl.json"
            },
            {
                "language": "ftl",
                "path": "./snippets/html.json"
            },
            {
                "language": "html",
                "path": "./snippets/html.json"
            },
            {
                "language": "html",
                "path": "./snippets/ftl.json"
            }
        ]
    },
    "bugs": {
        "url": "https://github.com/pschlup/vs-freemarker-2025/issues"
    },
    "homepage": "https://github.com/pschlup/vs-freemarker-2025#readme",
    "dependencies": {},
    "devDependencies": {},
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "visual",
        "studio",
        "code",
        "vs",
        "freemarker",
        "ftl"
    ],
    "author": "Paulo Schlup (https://github.com/pschlup)",
    "license": "MIT",
    "icon": "images/logo.png"
}
